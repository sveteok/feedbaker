{
  "name": "server",
  "version": "1.0.0",
  "description": "feedbaker server",
  "license": "ISC",
  "author": "Svetlana Teryaeva",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier . --check",
    "prettierfix": "prettier . --write",
    "start": "node ./dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "ts-node": "ts-node",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "build": "tsc --types node",
    "seed": "tsx src/scripts/seedData.ts",
    "install": "npm install --include=dev"
  },
  "dependencies": {
    "@google/genai": "^1.29.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.0",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vitest": "^3.2.4"
  }
}
